@tailwind base;
@tailwind components;
@tailwind utilities;

/* Import Inter font for modern typography */
@import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700;800;900&display=swap');
@import url('https://fonts.googleapis.com/css2?family=Orbitron:wght@400;500;600;700;800;900&display=swap');

/* CSS Variables for modern theming */
:root {
  --background: #000000;
  --foreground: #ffffff;
  --primary: #3b82f6;
  --primary-foreground: #ffffff;
  --secondary: #1f2937;
  --secondary-foreground: #f9fafb;
  --accent: #10b981;
  --accent-foreground: #ffffff;
  --muted: #374151;
  --muted-foreground: #9ca3af;
  --border: #374151;
  --input: #374151;
  --ring: #3b82f6;
  --radius: 0.75rem;
  --shadow: 0 25px 50px -12px rgba(0, 0, 0, 0.25);
  
  /* Futuristic color palette */
  --neon-blue: #00d4ff;
  --neon-purple: #8b5cf6;
  --neon-pink: #ec4899;
  --neon-green: #10b981;
  --neon-orange: #f59e0b;
  --neon-cyan: #06b6d4;
  --neon-yellow: #eab308;
  --neon-red: #ef4444;
  --quantum-glow: 0 0 20px rgba(0, 255, 255, 0.5);
  --holographic-glow: 0 0 30px rgba(139, 92, 246, 0.6);
  --cyberpunk-glow: 0 0 25px rgba(236, 73, 153, 0.7);
  --neural-pulse: 0 0 40px rgba(16, 185, 129, 0.4);
  --quantum-entanglement: 0 0 50px rgba(139, 92, 246, 0.3);
  --futuristic-shadow: 0 0 60px rgba(0, 255, 255, 0.3);
  --holographic-shadow: 0 0 80px rgba(139, 92, 246, 0.4);
  --cyberpunk-shadow: 0 0 70px rgba(236, 73, 153, 0.5);
  --quantum-field: 0 0 100px rgba(0, 255, 255, 0.1);
  --holographic-matrix: 0 0 120px rgba(139, 92, 246, 0.15);
  --neural-network: 0 0 90px rgba(16, 185, 129, 0.2);
  --quantum-ripple: 0 0 150px rgba(0, 255, 255, 0.08);
  --holographic-wave: 0 0 200px rgba(139, 92, 246, 0.12);
  --cyberpunk-pulse: 0 0 180px rgba(236, 73, 153, 0.1);
  
  /* New Enhanced Variables */
  --quantum-aura: 0 0 300px rgba(0, 255, 255, 0.05);
  --holographic-aura: 0 0 400px rgba(139, 92, 246, 0.08);
  --neural-aura: 0 0 350px rgba(16, 185, 129, 0.06);
  --cyberpunk-aura: 0 0 450px rgba(236, 73, 153, 0.07);
  --quantum-resonance: 0 0 600px rgba(0, 255, 255, 0.03);
  --holographic-resonance: 0 0 800px rgba(139, 92, 246, 0.04);
  --neural-resonance: 0 0 700px rgba(16, 185, 129, 0.03);
  --cyberpunk-resonance: 0 0 900px rgba(236, 73, 153, 0.04);
  
  /* 2026 Revolutionary Variables */
  --quantum-fusion: 0 0 1000px rgba(0, 255, 255, 0.02);
  --holographic-matrix-2026: 0 0 1200px rgba(139, 92, 246, 0.03);
  --neural-network-2026: 0 0 1100px rgba(16, 185, 129, 0.025);
  --cyberpunk-fusion-2026: 0 0 1300px rgba(236, 73, 153, 0.03);
  --space-tech-glow: 0 0 800px rgba(255, 69, 0, 0.04);
  --ai-consciousness: 0 0 900px rgba(138, 43, 226, 0.035);
  --quantum-space: 0 0 1400px rgba(0, 255, 255, 0.015);
  --holographic-events: 0 0 1000px rgba(255, 20, 147, 0.04);
}

* {
  box-sizing: border-box;
  padding: 0;
  margin: 0;
}

html,
body {
  max-width: 100vw;
  overflow-x: hidden;
  font-family: 'Inter', -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen, Ubuntu, Cantarell, 'Open Sans', 'Helvetica Neue', sans-serif;
  scroll-behavior: smooth;
}

body {
  color: rgb(var(--foreground-rgb));
  background: linear-gradient(
    135deg,
    rgb(var(--background-start-rgb)),
    rgb(var(--background-end-rgb)),
    rgb(30, 41, 59),
    rgb(15, 23, 42)
  );
  background-size: 400% 400%;
  min-height: 100vh;
  line-height: 1.6;
  animation: gradientShift 20s ease infinite;
}

/* Enhanced gradient animation */
@keyframes gradientShift {
  0% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
  100% { background-position: 0% 50%; }
}

/* Futuristic neon glow effects */
@keyframes neonGlow {
  0%, 100% { 
    box-shadow: 
      0 0 5px currentColor,
      0 0 10px currentColor,
      0 0 15px currentColor,
      0 0 20px currentColor;
  }
  50% { 
    box-shadow: 
      0 0 10px currentColor,
      0 0 20px currentColor,
      0 0 30px currentColor,
      0 0 40px currentColor;
  }
}

@keyframes neonPulse {
  0%, 100% { opacity: 1; }
  50% { opacity: 0.7; }
}

@keyframes float {
  0%, 100% { transform: translateY(0px); }
  50% { transform: translateY(-10px); }
}

@keyframes rotate3D {
  0% { transform: rotate3d(1, 1, 1, 0deg); }
  100% { transform: rotate3d(1, 1, 1, 360deg); }
}

@keyframes shimmer {
  0% { background-position: -200% 0; }
  100% { background-position: 200% 0; }
}

/* Futuristic background patterns */
.futuristic-bg {
  background: 
    radial-gradient(circle at 20% 80%, rgba(120, 119, 198, 0.3) 0%, transparent 50%),
    radial-gradient(circle at 80% 20%, rgba(255, 119, 198, 0.3) 0%, transparent 50%),
    radial-gradient(circle at 40% 40%, rgba(120, 219, 255, 0.2) 0%, transparent 50%);
  background-size: 100% 100%;
  animation: gradientShift 30s ease infinite;
}

/* Neon text effects */
.neon-text {
  text-shadow: 
    0 0 5px currentColor,
    0 0 10px currentColor,
    0 0 15px currentColor,
    0 0 20px currentColor;
  animation: neonPulse 2s ease-in-out infinite;
}

/* Futuristic card effects */
.futuristic-card {
  background: rgba(15, 23, 42, 0.8);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(59, 130, 246, 0.3);
  border-radius: 16px;
  transition: all 0.3s ease;
}

.futuristic-card:hover {
  border-color: rgba(59, 130, 246, 0.8);
  box-shadow: 
    0 0 20px rgba(59, 130, 246, 0.3),
    0 0 40px rgba(59, 130, 246, 0.1);
  transform: translateY(-5px);
}

/* Animated borders */
.animated-border {
  position: relative;
  background: linear-gradient(45deg, #3b82f6, #8b5cf6, #ec4899, #06b6d4);
  background-size: 400% 400%;
  animation: gradientShift 3s ease infinite;
  padding: 2px;
  border-radius: 16px;
}

.animated-border > * {
  background: rgba(15, 23, 42, 0.95);
  border-radius: 14px;
  padding: 1.5rem;
}

/* Responsive grid improvements */
@media (max-width: 768px) {
  .service-grid {
    grid-template-columns: 1fr;
    gap: 1.5rem;
  }
  
  .hero-stats {
    grid-template-columns: repeat(2, 1fr);
    gap: 1rem;
  }
}

@media (max-width: 480px) {
  .hero-stats {
    grid-template-columns: 1fr;
    gap: 1rem;
  }
}

/* Custom Scrollbar */
::-webkit-scrollbar {
  width: 12px;
}

::-webkit-scrollbar-track {
  background: rgba(0, 0, 0, 0.4);
  border-radius: 6px;
  border: 1px solid rgba(0, 255, 255, 0.2);
}

::-webkit-scrollbar-thumb {
  background: linear-gradient(45deg, var(--neon-cyan), var(--neon-purple));
  border-radius: 6px;
  border: 2px solid rgba(0, 0, 0, 0.3);
  box-shadow: var(--quantum-glow);
}

::-webkit-scrollbar-thumb:hover {
  background: linear-gradient(45deg, var(--neon-purple), var(--neon-pink));
  box-shadow: var(--holographic-glow);
}

/* Enhanced Selection */
::selection {
  background: rgba(0, 255, 255, 0.35);
  color: white;
  text-shadow: 0 0 10px rgba(0, 255, 255, 0.6);
}

::-moz-selection {
  background: rgba(0, 255, 255, 0.5);
  color: white;
  text-shadow: 0 0 15px rgba(0, 255, 255, 0.9);
}

/* 2026 Revolutionary Animations */
@keyframes quantum-pulse {
  0%, 100% { 
    box-shadow: var(--quantum-glow), var(--quantum-aura);
    transform: scale(1);
  }
  50% { 
