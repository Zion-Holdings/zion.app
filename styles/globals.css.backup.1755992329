/* Futuristic utility classes */
.neon-glow {
  animation: neonGlow 2s ease-in-out infinite;
}

.holographic {
  animation: hologram 3s ease-in-out infinite;
}

.quantum-float {
  animation: quantumFloat 4s ease-in-out infinite;
}

.cyber-grid {
  background-image: 
    linear-gradient(rgba(0, 255, 255, 0.1) 1px, transparent 1px),
    linear-gradient(90deg, rgba(0, 255, 255, 0.1) 1px, transparent 1px);
  background-size: 20px 20px;
  animation: cyberGrid 5s ease-in-out infinite;
}

/* Enhanced neon effects */
.neon-text {
  animation: neonPulse 2s ease-in-out infinite;
  text-shadow: 
    0 0 5px currentColor,
    0 0 10px currentColor,
    0 0 15px currentColor;
}

.neon-border {
  animation: neonBorder 3s ease-in-out infinite;
  border: 2px solid currentColor;
}

.neon-button {
  position: relative;
  overflow: hidden;
  transition: all 0.3s ease;
}

.neon-button::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255,255,255,0.2), transparent);
  transition: left 0.5s ease;
}

.neon-button:hover::before {
  left: 100%;
}

.neon-button:hover {
  transform: translateY(-2px);
  box-shadow: 
    0 0 10px currentColor,
    0 0 20px currentColor,
    0 0 30px currentColor;
}

/* Futuristic backgrounds */
.futuristic-bg {
  background: 
    radial-gradient(circle at 20% 80%, rgba(120, 119, 198, 0.3) 0%, transparent 50%),
    radial-gradient(circle at 80% 20%, rgba(255, 119, 198, 0.3) 0%, transparent 50%),
    radial-gradient(circle at 40% 40%, rgba(120, 219, 255, 0.3) 0%, transparent 50%);
  background-size: 200% 200%;
  animation: gradientShift 15s ease infinite;
}

.quantum-bg {
  background: 
    linear-gradient(45deg, #0f0f23 0%, #1a1a2e 25%, #16213e 50%, #0f3460 75%, #533483 100%);
  background-size: 400% 400%;
  animation: gradientShift 20s ease infinite;
}

/* Responsive design improvements */
@media (max-width: 768px) {
  .neon-text {
    animation: neonPulse 3s ease-in-out infinite;
  }
  
  .quantum-float {
    animation: quantumFloat 6s ease-in-out infinite;
  }
}

@media (max-width: 480px) {
  .neon-glow {
    animation: neonGlow 4s ease-in-out infinite;
  }
  
  .holographic {
    animation: hologram 5s ease-in-out infinite;
  }
}
