  animation: bounceGentle 3s ease-in-out infinite;
}

@keyframes bounceGentle {
  0%, 100% { transform: translateY(0); }
  50% { transform: translateY(-10px); }
}

/* Enhanced Grid Patterns */
.grid-pattern {
  background-image: 
    linear-gradient(rgba(0, 255, 255, 0.1) 1px, transparent 1px),
    linear-gradient(90deg, rgba(0, 255, 255, 0.1) 1px, transparent 1px);
  background-size: 20px 20px;
  animation: gridMove 20s linear infinite;
}

@keyframes gridMove {
  0% { transform: translate(0, 0); }
  100% { transform: translate(20px, 20px); }
}

/* Enhanced Particle Effects */
.particle-bg {
  position: relative;
  overflow: hidden;
}

.particle-bg::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-image: 
    radial-gradient(2px 2px at 20px 30px, rgba(0, 255, 255, 0.8), transparent),
    radial-gradient(2px 2px at 40px 70px, rgba(139, 92, 246, 0.8), transparent),
    radial-gradient(1px 1px at 90px 40px, rgba(236, 73, 153, 0.8), transparent),
    radial-gradient(1px 1px at 130px 80px, rgba(59, 130, 246, 0.8), transparent),
    radial-gradient(2px 2px at 160px 30px, rgba(16, 185, 129, 0.8), transparent);
  background-repeat: repeat;
  background-size: 200px 200px;
  animation: particleFloat 20s linear infinite;
  opacity: 0.3;
}

@keyframes particleFloat {
  0% { transform: translateY(0px); }
  100% { transform: translateY(-200px); }
}

/* Revolutionary new animations */
@keyframes quantumPulse {
  0%, 100% {
    box-shadow: 0 0 20px rgba(0, 255, 255, 0.5);
  }
  50% {
    box-shadow: 0 0 40px rgba(0, 255, 255, 0.8), 0 0 60px rgba(0, 255, 255, 0.4);
  }
}

@keyframes holographicMatrix {
  0% {
    background-position: 0% 0%;
  }
  50% {
    background-position: 100% 100%;
  }
  100% {
    background-position: 0% 0%;
  }
}

@keyframes neuralNetwork {
  0%, 100% {
    opacity: 0.3;
    transform: scale(1);
  }
  50% {
    opacity: 0.8;
    transform: scale(1.1);
  }
}

@keyframes cyberpunkGlitch {
  0%, 100% {
    transform: translateX(0);
  }
  10% {
    transform: translateX(-2px);
  }
  20% {
    transform: translateX(2px);
  }
  30% {
    transform: translateX(-1px);
  }
  40% {
    transform: translateX(1px);
  }
}

@keyframes quantumEntanglement {
  0% {
    transform: rotate(0deg) scale(1);
  }
  25% {
    transform: rotate(90deg) scale(1.2);
  }
  50% {
    transform: rotate(180deg) scale(0.8);
  }
  75% {
    transform: rotate(270deg) scale(1.1);
  }
  100% {
    transform: rotate(360deg) scale(1);
  }
}

@keyframes aiFuturistic {
  0%, 100% {
    filter: hue-rotate(0deg) brightness(1);
  }
  50% {
    filter: hue-rotate(180deg) brightness(1.2);
  }
}

@keyframes quantumSpace {
  0% {
    transform: translateY(0) scale(1);
  }
  50% {
    transform: translateY(-20px) scale(1.1);
  }
  100% {
    transform: translateY(0) scale(1);
  }
}

/* Enhanced Responsive Design */
@media (max-width: 768px) {
  .card-futuristic:hover {
    transform: none;
  }
  
  .btn-futuristic:hover {
    transform: none;
  }
  
  .particle-bg::before {
    background-size: 100px 100px;
  }
}

/* Enhanced Loading States */
.loading-pulse {
  animation: loadingPulse 1.5s ease-in-out infinite;
}

@keyframes loadingPulse {
  0%, 100% { opacity: 1; }
  50% { opacity: 0.5; }
}

/* Enhanced Focus States */
.focus-ring:focus {
  outline: none;
  box-shadow: 0 0 0 3px rgba(0, 255, 255, 0.3);
}

/* Enhanced Hover Effects */
.hover-lift {
  transition: transform 0.3s ease, box-shadow 0.3s ease;
}

.hover-lift:hover {
  transform: translateY(-5px);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
}

/* Enhanced Text Shadows */
.text-shadow-neon {
  text-shadow: 0 0 10px var(--neon-cyan);
}

.text-shadow-holographic {
  text-shadow: 0 0 15px var(--neon-purple);
}

.text-shadow-cyberpunk {
  text-shadow: 0 0 12px var(--neon-pink);
}

/* Enhanced Border Gradients */
.border-gradient-cyan {
  border: 2px solid transparent;
  background: linear-gradient(white, white) padding-box,
              linear-gradient(135deg, var(--neon-cyan), var(--neon-blue)) border-box;
}

.border-gradient-purple {
  border: 2px solid transparent;
  background: linear-gradient(white, white) padding-box,
              linear-gradient(135deg, var(--neon-purple), var(--neon-pink)) border-box;
}

/* Enhanced Glass Morphism */
.glass-morphism {
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.2);
  border-radius: 16px;
}

/* Enhanced Neon Text */
.neon-text-cyan {
  color: var(--neon-cyan);
  text-shadow: 
    0 0 5px var(--neon-cyan),
    0 0 10px var(--neon-cyan),
    0 0 15px var(--neon-cyan),
    0 0 20px var(--neon-cyan);
}

.neon-text-purple {
  color: var(--neon-purple);
  text-shadow: 
    0 0 5px var(--neon-purple),
    0 0 10px var(--neon-purple),
    0 0 15px var(--neon-purple),
    0 0 20px var(--neon-purple);
}

/* Enhanced Quantum Effects */
.quantum-border {
  position: relative;
  border: 2px solid transparent;
  background: linear-gradient(45deg, var(--neon-cyan), var(--neon-purple), var(--neon-pink));
  background-size: 200% 200%;
  animation: quantumShift 3s ease-in-out infinite;
}

@keyframes quantumShift {
  0%, 100% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
}

/* Enhanced Holographic Effects */
.holographic-border {
  position: relative;
  border: 2px solid transparent;
  background: linear-gradient(45deg, var(--neon-purple), var(--neon-pink), var(--neon-cyan));
  background-size: 300% 300%;
  animation: holographicShift 4s ease-in-out infinite;
}

@keyframes holographicShift {
  0%, 100% { background-position: 0% 0%; }
  25% { background-position: 100% 0%; }
  50% { background-position: 100% 100%; }
  75% { background-position: 0% 100%; }
}

/* Enhanced Neural Network Effects */
.neural-network {
  background-image: 
    radial-gradient(circle at 25% 25%, rgba(16, 185, 129, 0.3) 0%, transparent 50%),
    radial-gradient(circle at 75% 75%, rgba(16, 185, 129, 0.3) 0%, transparent 50%);
  animation: neuralFlow 5s ease-in-out infinite;
}

@keyframes neuralFlow {
  0%, 100% { opacity: 0.3; }
  50% { opacity: 0.8; }
}

/* Enhanced Space Effects */
.space-bg {
  background-image: 
    radial-gradient(circle at 20% 80%, rgba(59, 130, 246, 0.2) 0%, transparent 50%),
    radial-gradient(circle at 80% 20%, rgba(139, 92, 246, 0.2) 0%, transparent 50%);
  animation: spaceFloat 15s ease-in-out infinite;
}

@keyframes spaceFloat {
  0%, 100% { transform: scale(1) rotate(0deg); }
  50% { transform: scale(1.1) rotate(1deg); }
}

/* Enhanced Biotech Effects */
.biotech-glow {
  box-shadow: var(--biotech-glow);
  transition: all 0.3s ease;
}

.biotech-glow:hover {
  box-shadow: 0 0 50px rgba(16, 185, 129, 0.8);
  transform: scale(1.05);
}

/* Enhanced Metaverse Effects */
.metaverse-glow {
  box-shadow: var(--metaverse-glow);
  transition: all 0.3s ease;
}

.metaverse-glow:hover {
  box-shadow: 0 0 60px rgba(139, 92, 246, 0.8);
  transform: scale(1.05);
}

/* Enhanced Financial Effects */
.financial-glow {
  box-shadow: 0 0 30px rgba(245, 158, 11, 0.5);
  transition: all 0.3s ease;
}

.financial-glow:hover {
  box-shadow: 0 0 50px rgba(245, 158, 11, 0.8);
  transform: scale(1.05);
}

/* Enhanced Logistics Effects */
.logistics-glow {
  box-shadow: 0 0 30px rgba(20, 184, 166, 0.5);
  transition: all 0.3s ease;
}

.logistics-glow:hover {
  box-shadow: 0 0 50px rgba(20, 184, 166, 0.8);
  transform: scale(1.05);
}

/* Enhanced Energy Effects */
.energy-glow {
  box-shadow: 0 0 30px rgba(16, 185, 129, 0.5);
  transition: all 0.3s ease;
}

.energy-glow:hover {
  box-shadow: 0 0 50px rgba(16, 185, 129, 0.8);
  transform: scale(1.05);
}

/* Enhanced Education Effects */
.education-glow {
  box-shadow: 0 0 30px rgba(99, 102, 241, 0.5);
  transition: all 0.3s ease;
}

.education-glow:hover {
  box-shadow: 0 0 50px rgba(99, 102, 241, 0.8);
  transform: scale(1.05);
}

/* Enhanced Responsive Utilities */
@media (max-width: 640px) {
  .text-gradient-rainbow {
    background: linear-gradient(135deg, var(--neon-cyan), var(--neon-blue));
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }
  
  .particle-bg::before {
    background-size: 50px 50px;
  }
}

@media (max-width: 1024px) {
  .quantum-border,
  .holographic-border {
    animation: none;
  }
}

/* Enhanced Accessibility */
@media (prefers-reduced-motion: reduce) {
  *,
  *::before,
  *::after {
    animation-duration: 0.01ms !important;
    animation-iteration-count: 1 !important;
    transition-duration: 0.01ms !important;
  }
}

/* Enhanced Print Styles */
@media print {
  .quantum-glow,
  .holographic-glow,
  .cyberpunk-glow,
  .neural-pulse,
  .quantum-entanglement {
    box-shadow: none;
  }
}

/* Enhanced Utility Classes */
.hover-glow:hover {
  animation: glow 2s ease-in-out infinite alternate;
}

.hover-pulse:hover {
  animation: pulse-glow 2s ease-in-out infinite;
}

.neon-flicker {
  animation: neon-flicker 3s infinite alternate;
}

.hologram-effect {
  animation: hologram 4s ease-in-out infinite;
}

.cyber-pulse {
  background: linear-gradient(-45deg, #3b82f6, #8b5cf6, #ec4899, #10b981);
  background-size: 400% 400%;
  animation: cyber-pulse 3s ease infinite;
}

.quantum-float {
  animation: quantum-float 6s ease-in-out infinite;
}

/* Enhanced Background Patterns */
.futuristic-bg {
  background-image: 
    radial-gradient(circle at 25% 25%, rgba(59, 130, 246, 0.1) 0%, transparent 50%),
    radial-gradient(circle at 75% 75%, rgba(139, 92, 246, 0.1) 0%, transparent 50%),
    linear-gradient(45deg, transparent 40%, rgba(59, 130, 246, 0.05) 50%, transparent 60%);
  background-size: 100% 100%, 100% 100%, 20px 20px;
}

.cyber-grid {
  background-image: 
    linear-gradient(rgba(59, 130, 246, 0.1) 1px, transparent 1px),
    linear-gradient(90deg, rgba(59, 130, 246, 0.1) 1px, transparent 1px);
  background-size: 30px 30px;
}

.data-stream-bg {
  background-image: 
    linear-gradient(90deg, transparent 0%, rgba(59, 130, 246, 0.1) 50%, transparent 100%);
  background-size: 200% 100%;
  animation: data-stream 10s linear infinite;
}

.matrix-bg {
  background-image: 
    linear-gradient(180deg, transparent 0%, rgba(16, 185, 129, 0.1) 50%, transparent 100%);
  background-size: 100% 200%;
  animation: matrix-rain 20s linear infinite;
}

/* Enhanced Glass Effects */
.glass-morphism {
  background: rgba(255, 255, 255, 0.05);
  backdrop-filter: blur(20px);
  border: 1px solid rgba(255, 255, 255, 0.1);
  box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3);
}

.glass-morphism-hover:hover {
  background: rgba(255, 255, 255, 0.1);
  border-color: rgba(59, 130, 246, 0.5);
  box-shadow: 0 8px 32px rgba(59, 130, 246, 0.2);
}

/* Enhanced Button Styles */
.btn-futuristic {
  background: linear-gradient(135deg, #3b82f6, #8b5cf6);
  border: none;
  color: white;
  padding: 12px 24px;
  border-radius: 8px;
  font-weight: 600;
  transition: all 0.3s ease;
  position: relative;
  overflow: hidden;
}

.btn-futuristic::before {
  content: '';
  position: absolute;
  top: 0;
  left: -100%;
  width: 100%;
  height: 100%;
  background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
  transition: left 0.5s;
}

.btn-futuristic:hover::before {
  left: 100%;
}

.btn-futuristic:hover {
  transform: translateY(-2px);
  box-shadow: 0 10px 25px rgba(59, 130, 246, 0.4);
}

/* Enhanced Card Effects */
.card-hover {
  transition: all 0.3s ease;
}

.card-hover:hover {
  transform: translateY(-8px) scale(1.02);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.3);
}

.card-glow {
  position: relative;
}

.card-glow::after {
  content: '';
  position: absolute;
  top: -2px;
  left: -2px;
  right: -2px;
  bottom: -2px;
  background: linear-gradient(45deg, #3b82f6, #8b5cf6, #ec4899, #10b981);
  border-radius: inherit;
  z-index: -1;
  opacity: 0;
  transition: opacity 0.3s ease;
}

.card-glow:hover::after {
  opacity: 1;
}

/* Enhanced Text Effects */
.text-gradient {
  background: linear-gradient(135deg, #3b82f6, #8b5cf6, #ec4899);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.text-glow {
  text-shadow: 0 0 10px rgba(59, 130, 246, 0.5);
}

.text-neon {
  color: #3b82f6;
  text-shadow: 
    0 0 5px #3b82f6,
    0 0 10px #3b82f6,
    0 0 15px #3b82f6,
    0 0 20px #3b82f6;
}

/* Enhanced Navigation */
.nav-glass {
  background: rgba(0, 0, 0, 0.8);
  backdrop-filter: blur(20px);
  border-bottom: 1px solid rgba(255, 255, 255, 0.1);
}

.nav-link {
  position: relative;
  transition: color 0.3s ease;
}

.nav-link::after {
  content: '';
  position: absolute;
  bottom: -5px;
  left: 0;
  width: 0;
  height: 2px;
  background: linear-gradient(90deg, #3b82f6, #8b5cf6);
  transition: width 0.3s ease;
}

.nav-link:hover::after {
  width: 100%;
}

/* Enhanced Form Elements */
.input-futuristic {
  background: rgba(255, 255, 255, 0.05);
  border: 1px solid rgba(255, 255, 255, 0.1);
  border-radius: 8px;
  padding: 12px 16px;
  color: white;
  transition: all 0.3s ease;
}

.input-futuristic:focus {
  outline: none;
  border-color: #3b82f6;
  box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.1);
  background: rgba(255, 255, 255, 0.1);
}

/* Enhanced Loading States */
.loading-pulse {
  animation: pulse 2s cubic-bezier(0.4, 0, 0.6, 1) infinite;
}

.loading-spin {
  animation: spin 1s linear infinite;
}

@keyframes pulse {
  0%, 100% { opacity: 1; }
  50% { opacity: 0.5; }
}

@keyframes spin {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

/* Enhanced Scrollbar */
::-webkit-scrollbar {
  width: 8px;
}

::-webkit-scrollbar-track {
  background: rgba(0, 0, 0, 0.1);
}

::-webkit-scrollbar-thumb {
  background: linear-gradient(180deg, #3b82f6, #8b5cf6);
  border-radius: 4px;
}

::-webkit-scrollbar-thumb:hover {
  background: linear-gradient(180deg, #2563eb, #7c3aed);
}

/* Enhanced Selection */
::selection {
  background: rgba(59, 130, 246, 0.3);
  color: white;
}

::-moz-selection {
  background: rgba(59, 130, 246, 0.3);
  color: white;
}

/* Enhanced Focus States */
.focus-ring:focus {
  outline: none;
  box-shadow: 0 0 0 3px rgba(59, 130, 246, 0.5);
}

/* Enhanced Transitions */
.transition-all {
  transition: all 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

.transition-transform {
  transition: transform 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

.transition-colors {
  transition: color 0.3s cubic-bezier(0.4, 0, 0.2, 1), background-color 0.3s cubic-bezier(0.4, 0, 0.2, 1), border-color 0.3s cubic-bezier(0.4, 0, 0.2, 1);
}

/* Enhanced Shadows */
.shadow-futuristic {
  box-shadow: 
    0 10px 25px rgba(0, 0, 0, 0.2),
    0 4px 10px rgba(0, 0, 0, 0.1);
}

.shadow-glow {
  box-shadow: 
    0 0 20px rgba(59, 130, 246, 0.3),
    0 0 40px rgba(59, 130, 246, 0.2);
}

.shadow-neon {
  box-shadow: 
    0 0 5px rgba(59, 130, 246, 0.5),
    0 0 10px rgba(59, 130, 246, 0.3),
    0 0 15px rgba(59, 130, 246, 0.1);
}
