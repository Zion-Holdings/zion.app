# General
ENV=dev
LOG_LEVEL=INFO

# Model provider
MODEL_PROVIDER=openai  # options: openai, local
OPENAI_API_KEY=
OPENAI_FINE_TUNE_ORG=

# Default model choices
CHAT_MODEL=gpt-4o-mini
EMBEDDING_MODEL=text-embedding-3-small

# Database & storage
DATABASE_URL=sqlite:///./app/storage/zion_core.db
STORAGE_DIR=./app/storage

# Admin
ADMIN_RATE_LIMIT_RPM=60

# Scheduler
SCHEDULER_ENABLED=0
SCHEDULE_CRON="0 30 0 1 * *"  # minute hour day-of-month month day-of-week year (informational)