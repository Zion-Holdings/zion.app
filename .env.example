# Copy this file to .env and fill in values
OPENAI_API_KEY=sk-REPLACE_ME
# Optional: override model (defaults to gpt-4o-mini). Examples: gpt-4o, gpt-4-turbo, gpt-4.1-mini
OPENAI_MODEL=gpt-4o-mini
# CORS allowlist for embedding; use * for same-origin dev
ALLOWED_ORIGIN=*
# PORT to run the server on
PORT=3000