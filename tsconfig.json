{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "esnext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true
  },
  "include": ["src/App.tsx", "src/main.tsx", "src/index.css", "src/pages/Home.tsx", "src/pages/Services.tsx", "src/pages/About.tsx", "src/pages/Contact.tsx"],
  "exclude": ["node_modules", "src/components", "src/layout", "src/hooks", "src/lib", "src/data", "src/types", "src/pages/Blog.tsx", "src/pages/Careers.tsx", "src/pages/FAQ.tsx", "src/pages/GreenIT.tsx", "src/pages/HelpCenter.tsx", "src/pages/Privacy.tsx", "src/pages/Sitemap.tsx", "src/pages/Terms.tsx", "src/pages/talent"]
}
