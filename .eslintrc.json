{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "warn",
    "prefer-const": "error",
    "no-var": "error",
    "eqeqeq": "error",
    "no-duplicate-imports": "error"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "dist/",
    "build/",
    "*.min.js",
    "public/",
    "temp_essential_pages/",
    "__tests__/",
    "cypress/",
    "scripts/",
    "backend/"
  ]
} 