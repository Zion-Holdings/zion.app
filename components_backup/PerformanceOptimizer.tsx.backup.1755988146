      document.documentElement.style.setProperty('--offline-mode', 'enabled');
      newOptimizations.push('Enabled offline caching');
    }
    
    setOptimizations(newOptimizations);
  }, [metrics, targetFPS]);

  // Auto-optimization
  useEffect(() => {
