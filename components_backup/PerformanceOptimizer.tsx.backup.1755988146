      }, 2000);
      
      return () => clearTimeout(timer);
    }
  }, [performanceIssues, enableOptimizations, applyOptimizations]);

  // Initialize performance monitoring
  useEffect(() => {
    if (typeof window === 'undefined') return;
    
    // Start FPS monitoring
