steps:
      - name: "Agent: agent-1755379777754-6-backend-expert"
        run: echo "Step completed"

on:
  workflow_dispatch:
  schedule:
    - cron: 0 */6 * * *

concurrency:
  group: "github.workflow-${{ github.ref }}"
  cancel-in-progress: true

jobs:
  agent-execution:
        - name: Checkout code
        uses: actions/checkout@v4
        - name: Setup Node.js
        uses: actions/setup-node@v4
        node-version: 18
        - name: Install dependencies
        run: npm ci
        - name: Run agent
        run: node automation/agents/backend/agent-1755379777754-6-backend-expert/index.cjs
