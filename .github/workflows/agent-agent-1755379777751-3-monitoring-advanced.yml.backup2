name: "Agent: agent-1755379777751-3-monitoring-advanced"
on:
  workflow_dispatch:
  schedule:
    - cron: 0 */6 * * *

concurrency:
  group: "github.workflow-${{ github.ref }}"
  cancel-in-progress: true

jobs:
  agent-execution:
        - name: Checkout code
        uses: actions/checkout@v4
        - name: Setup Node.js
        uses: actions/setup-node@v4
        node-version: 18
        - name: Install dependencies
        run: npm ci
        - name: Run agent
        run: node automation/agents/monitoring/agent-1755379777751-3-monitoring-advanced/index.cjs
