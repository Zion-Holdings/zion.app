  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+zion",
      "url": "/?protocol=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/file-handler",
      "accept": {
        "text/*": [".txt", ".md", ".json"],
        "application/json": [".json"],
        "image/*": [".png", ".jpg", ".jpeg", ".gif", ".webp"]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "files",
          "accept": ["image/*", "text/*", "application/*"]
        }
      ]
    }
  },
  "permissions": [
    "notifications",
    "geolocation",
    "camera",
    "microphone"
  ],
  "features": [
    "Cross Platform",
    "fast",
    "simple",
    "Offline Support",
    "Push Notifications",
    "Background Sync"
  ]
