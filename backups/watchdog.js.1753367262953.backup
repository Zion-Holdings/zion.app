function logErrorToProduction(message, errorObject = null) {
  const timestamp = new Date().toISOString();
  logger.error(`[${timestamp}] WATCHDOG ERROR: ${message}`);
  if (errorObject) {
    logger.error(errorObject.message);
    if (errorObject.stack) {
