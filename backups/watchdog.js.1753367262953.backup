async function sendDiscordAlert(alertMessage) {
  if (!DISCORD_WEBHOOK_URL) {
    const logMsg = Discord Webhook URL not configured (DISCORD_WEBHOOK_URL environment variable is not set). Skipping alert.';    logger.warn(logMsg);
