{
  "autoCreateAgents": true,
  "autoCreateFactories": true,
  "autoImproveAgents": true,
  "autoImproveFactories": true,
  "autoMonitorSystem": true,
  "autoScaleSystem": true,
  "agentCreationInterval": 180000,
  "factoryCreationInterval": 300000,
  "improvementInterval": 120000,
  "monitoringInterval": 15000,
  "scalingInterval": 180000,
  "maxAgents": 100,
  "maxFactories": 50,
  "maxAutomations": 200,
  "performanceThreshold": 0.85,
  "errorThreshold": 0.05,
  "autoCommit": true,
  "autoDeploy": false,
  "backupInterval": 1800000,
  "cleanupInterval": 43200000,
  "analyticsEnabled": true,
  "reportingEnabled": true,
  "continuousLearning": true,
  "adaptiveOptimization": true,
  "requiredAgents": [
    "component-sync",
    "page-sync",
    "api-sync",
    "test-sync",
    "build-sync",
    "deployment-sync",
    "performance-sync",
    "security-sync",
    "quality-sync",
    "compliance-sync"
  ],
  "requiredFactories": [
    "frontend-sync",
    "component-generation",
    "page-generation",
    "api-generation",
    "test-generation",
    "build-optimization",
    "deployment-automation",
    "performance-monitoring",
    "security-scanning",
    "quality-assurance"
  ],
  "monitoring": {
    "enabled": true,
    "interval": 15000,
    "alerts": {
      "memoryThreshold": 80,
      "cpuThreshold": 80,
      "diskThreshold": 90,
      "errorThreshold": 5
    }
  },
  "backup": {
    "enabled": true,
    "interval": 1800000,
    "retentionDays": 7,
    "includeLogs": true,
    "includeAnalytics": true
  },
  "analytics": {
    "enabled": true,
    "collectionInterval": 30000,
    "retentionDays": 30,
    "metrics": [
      "system",
      "process",
      "automation",
      "performance",
      "errors"
    ]
  },
  "learning": {
    "enabled": true,
    "interval": 300000,
    "dataRetentionDays": 90,
    "improvementThreshold": 0.1
  },
  "optimization": {
    "enabled": true,
    "interval": 300000,
    "strategies": [
      "memory",
      "cpu",
      "network",
      "disk"
    ]
  },
  "scaling": {
    "enabled": true,
    "interval": 180000,
    "autoScale": true,
    "maxInstances": 10,
    "minInstances": 1
  },
  "security": {
    "enabled": true,
    "scanInterval": 3600000,
    "vulnerabilityChecks": true,
    "dependencyAudits": true
  },
  "quality": {
    "enabled": true,
    "checkInterval": 600000,
    "codeQuality": true,
    "testCoverage": true,
    "performanceMetrics": true
  },
  "deployment": {
    "enabled": false,
    "autoDeploy": false,
    "deploymentStrategy": "manual",
    "environments": [
      "development",
      "staging",
      "production"
    ]
  },
  "notifications": {
    "enabled": true,
    "channels": [
      "log",
      "email",
      "slack"
    ],
    "levels": [
      "error",
      "warning",
      "info"
    ]
  },
  "logging": {
    "level": "info",
    "format": "json",
    "rotation": {
      "enabled": true,
      "maxSize": "100MB",
      "maxFiles": 10
    }
  },
  "cron": {
    "enabled": true,
    "jobs": [
      {
        "name": "continuous-creation",
        "schedule": "*/3 * * * *",
        "enabled": true
      },
      {
        "name": "continuous-improvement",
        "schedule": "*/2 * * * *",
        "enabled": true
      },
      {
        "name": "performance-optimization",
        "schedule": "*/5 * * * *",
        "enabled": true
      },
      {
        "name": "error-recovery",
        "schedule": "*/1 * * * *",
        "enabled": true
      },
      {
        "name": "health-check",
        "schedule": "*/1 * * * *",
        "enabled": true
      },
      {
        "name": "analytics-collection",
        "schedule": "*/1 * * * *",
        "enabled": true
      },
      {
        "name": "system-backup",
        "schedule": "0 */6 * * *",
        "enabled": true
      },
      {
        "name": "cleanup",
        "schedule": "0 2 * * *",
        "enabled": true
      }
    ]
  }
} 