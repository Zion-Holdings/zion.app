{
  "name": "zion-app-automation-system",
  "version": "2.0.0",
  "description": "Enhanced AI-Powered Automation System for Zion App",
  "main": "enhanced-automation-system.js",
  "scripts": {
    "start": "node enhanced-automation-system.js",
    "stop": "node automation-manager.js stop",
    "restart": "node automation-manager.js restart",
    "status": "node automation-manager.js status",
    "logs": "node automation-manager.js logs",
    "dashboard": "node automation-manager.js dashboard",
    "test": "node automation-manager.js test",
    "setup": "node automation-manager.js setup",
    "clean": "node automation-manager.js clean",
    "help": "node automation-manager.js help",
    "monitor": "node performance/monitor.js start",
    "fix": "node netlify-error-fixer.js fix",
    "fix-all": "node netlify-error-fixer.js fix all",
    "prebuild": "node netlify-error-fixer.js fix dependency-error typescript-error eslint-error",
    "postbuild": "node netlify-monitor.js check",
    "setup": "npm install && node automation-manager.js setup",
    "test": "node test-automation.js",
    "logs": "tail -f automation.log",
    "clean": "rm -f *.log *.json && echo 'Cleaned automation logs and status files'"
  },
  "keywords": [
    "automation",
    "ai",
    "continuous-improvement",
    "monitoring",
    "optimization",
    "cursor-ai",
    "openai",
    "claude"
  ],
  "author": "Zion App Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "node-fetch": "^3.3.2",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "table": "^6.8.1",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "winston": "^3.10.0",
    "node-cron": "^3.0.2",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "eslint": "^8.50.0",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/bolt.new.zion.app.git"
  },
