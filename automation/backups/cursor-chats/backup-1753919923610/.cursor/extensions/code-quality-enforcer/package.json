{
  "name": "cursor-code-quality-enforcer",
  "version": "1.0.0",
  "description": "Advanced code quality enforcement extension for Cursor with automated linting, formatting, and improvement suggestions",
  "main": "dist/extension.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "dev": "npm run build && node dist/extension.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@types/node": "^20.8.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "chokidar": "^3.5.3",
    "glob": "^10.3.10",
    "fs-extra": "^11.2.0",
    "simple-git": "^3.22.0",
    "execa": "^7.2.0",
    "ora": "^8.0.1",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.15",
    "node-cron": "^3.0.3",
    "ws": "^8.16.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "eslint-plugin-security": "^2.1.0"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cursor",
    "code-quality",
    "linting",
    "formatting",
    "eslint",
    "prettier",
    "typescript",
    "react",
    "nextjs"
  ],
  "author": "Code Quality Enforcer",
  "license": "MIT"
}
